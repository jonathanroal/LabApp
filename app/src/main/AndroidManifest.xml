<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.intec.labapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/labapp_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/labapp_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity
            android:name=".LogView"
            android:label="@string/title_activity_log_view"
            android:theme="@style/NoActionBar"></activity>
        <activity android:name=".RequestLogs" />
        <activity
            android:name=".LogScreen"
            android:label="@string/title_activity_log_screen"
            android:theme="@style/NoActionBar" />
        <activity android:name=".RequestInstallation" />
        <activity
            android:name=".OperatorMenu"
            android:label="@string/title_activity_operator_menu"
            android:theme="@style/NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>